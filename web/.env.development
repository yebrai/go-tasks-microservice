# =============================================================================
# Frontend Development Environment Variables
# =============================================================================
# This file is used during development with `npm run dev`

# API Configuration
VITE_API_URL=http://localhost:8080/api
VITE_WS_URL=ws://localhost:8080/ws

# External Services URLs
VITE_RABBITMQ_MANAGEMENT_URL=http://localhost:15672

# Development Features
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_DATA=true
VITE_LOG_LEVEL=debug

# UI Configuration
VITE_APP_TITLE=Tasks Microservice Dashboard (Dev)
VITE_ENABLE_DEVTOOLS=true

# Polling/Refresh Intervals (milliseconds)
VITE_HEALTH_CHECK_INTERVAL=30000
VITE_EVENT_POLLING_INTERVAL=5000